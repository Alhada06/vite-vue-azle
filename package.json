{
  "name": "vite-vue-azle",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "setup": "npm i && dfx canister create backend && run-p setup:ledger && dfx generate backend && dfx deploy ",
    "setup:ic": "npm i && dfx canister create backend && run-p setup:ledger_ic && dfx generate backend && dfx deploy  --network ic",
    "setup:ledger": "./ledger_local.sh",
    "setup:ledger_ic": "./ledger_ic.sh",
    "start": "run-p frontend",
    "frontend": "vite --port 3000",
    "backend": "dfx deploy backend",
    "build": "tsc && vite build",
    "pretest:backend": "AZLE_REBUILD=true ts-node -P tsconfig.node.json --transpile-only --ignore=false backend/tests/pretest.ts",
    "test:backend": "AZLE_REBUILD=true ts-node -P tsconfig.node.json --transpile-only --ignore=false backend/tests/test.ts",
    "format": "prettier --write \"src/**/*.{json,js,jsx,ts,tsx,css,scss}\" "
  },
  "dependencies": {
    "@dfinity/auth-client": "^0.18.1",
    "@dfinity/identity": "^0.18.1",
    "@vueuse/core": "^10.3.0",
    "azle": "^0.17.1",
    "nanoid": "^4.0.2",
    "pinia": "^2.1.6",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@iconify-json/mdi": "^1.1.54",
    "@iconify-json/svg-spinners": "^1.1.2",
    "@tailwindcss/typography": "^0.5.9",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@vitejs/plugin-react": "^4.0.1",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/compiler-sfc": "^3.3.4",
    "@vuedx/typescript-plugin-vue": "^0.7.6",
    "autoprefixer": "^10.4.15",
    "bignumber.js": "^9.1.1",
    "cross-fetch": "^3.1.6",
    "daisyui": "^3.6.1",
    "dotenv": "^16.3.1",
    "jsdom": "^22.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.28",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "sass": "^1.63.6",
    "tailwindcss": "^3.3.3",
    "ts-node": "10.7.0",
    "typescript": "^5.1.6",
    "unplugin-auto-import": "^0.16.6",
